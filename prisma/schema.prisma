datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model TestProfile {
id  String @id @default(uuid())
name String
}

model PostTable {
  id  String @id @default(uuid())
  language String
  title String
  content String
  like Int
  unlike Int
  User String
}
